{
  "name": "@questdb/sql-grammar",
  "version": "1.0.12",
  "description": "List of keywords, functions and constants that QuestDB supports",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/*.ts\"",
    "lint": "eslint 'src/*.ts' --max-warnings=0 --config .eslintrc.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/questdb/sql-grammar.git"
  },
  "keywords": [
    "QuestDB"
  ],
  "author": "QuestDB",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/questdb/sql-grammar/issues"
  },
  "homepage": "https://github.com/questdb/sql-grammar#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "prettier": "2.0.5",
    "typescript": "3.9.6"
  }
}
